class Main Inherits IO {
    boolValue : Bool;
    intValue : Int;
    add : Int;

    main: Object;
    main() : Object {
        {
            let x: Int <- 5 in x

            let vacia : Int in vacia
            let math: Int <- new MathOperations IN math;
            vacia <- math.add(x,4,8);
            IO.print(vacia)

        }
    };
};

class MathOperations {
    add : Int;
    add(a: Int, b: Int, c: Int): Int {
        a * (b - a) + b - c
    }
}